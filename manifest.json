{
    "manifest_version": 4,
    "type": "extension",
    "name": "PluginInstaller",
    "slug": "plugininstaller",
    "description": "插件安装器",
    "icon": "./static/icon.png",
    "thumb": "./static/thumb.svg",
    "version": "1.2.4",
    "authors": [
        {
            "name": "xinyihl",
            "link": "https://github.com/xinyihl"
        }
    ],
    "repository": {
        "repo": "xinyihl/LiteLoaderQQNT-PluginInstaller",
        "branch": "main",
        "release": {
            "tag": "v1.2.4",
            "file": "Plugininstaller-release.zip"
        }
